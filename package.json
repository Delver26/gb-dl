{
  "name": "gb-dl",
  "version": "2.3.0",
  "description": "A CLI for downloading Giant Bomb videos.",
  "bin": "./bin/bin.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "giant bomb",
    "downloader",
    "cli"
  ],
  "engines": {
    "node": ">=8.11.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lightpohl/gb-dl.git"
  },
  "files": [
    "bin"
  ],
  "author": "Joshua Pohl",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^6.5.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  },
  "dependencies": {
    "commander": "^4.0.0",
    "dayjs": "^1.8.17",
    "filenamify": "^4.1.0",
    "got": "^9.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  }
}
