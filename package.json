  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-prettier": "^6.5.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  },
  "dependencies": {
    "commander": "^4.0.0",
    "got": "^9.6.0"
  }
